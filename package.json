    "animate.css": "^3.7.2",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.1.0",
    "cookie-universal-nuxt": "^2.1.0",
    "cross-env": "^5.2.1",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "nuxt": "^2.10.2",
    "nuxt-simple-line-icons": "^1.0.2",
    "vuejs-datepicker": "^1.6.2",
    "vuex-easy-access": "^3.1.8"
