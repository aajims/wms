    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.3.1",
    "stylelint": "^10.1.0",
